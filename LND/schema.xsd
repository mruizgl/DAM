 <xs:element name="alumno">
<xs:complexType>
  <xs:sequence>
   <xs:element ref="alumno" minOccurs="1" maxOccurs="unbounded"></xs:element> 

  
  </xs:sequence>
</xs:complexType>
 </xs:element>


    <xs:complexType>
